@model IEnumerable<ObjetoBase>

<table class="table table-hover">
<thead>
    <tr>
        <th scope="col" style="width: 400px;">Id</th>
        <th scope="col">Nombre</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nombre</td>
        </tr>
    }
</tbody>
</table>