@model IEnumerable<Evaluacion>

<table class="table table-dark table-hover">
<thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Evaluacion</th>
        <th scope="col">Alumno</th>
        <th scope="col">Calificacion</th>
    </tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nombre</td>
            <td>@item.AlumnoId</td>
            <td>@item.Calificacion</td>
        </tr>
    }
</tbody>
</table>